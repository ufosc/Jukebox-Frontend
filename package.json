    "eslint": "^9.9.1",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prefer-arrow": "^1.2.3",
    "eslint-plugin-prettier": "^5.1.0",
    "eslint-plugin-react": "^7.34.1",
    "eslint-plugin-react-refresh": "^0.4.4",
    "fake-indexeddb": "^6.0.0",
    "jsdom": "^25.0.1",
    "jukebox-types": "^0.1.3",
    "prettier": "^3.1.1",
    "sass": "^1.69.5",
    "typescript": "^5.2.2",
    "typescript-eslint": "^8.4.0",
    "vite": "^6.3.5",
